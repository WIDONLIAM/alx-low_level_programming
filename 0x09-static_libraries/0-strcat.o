#include "main.h"

 /**
 * _strcat - Will be concatenating two strings
 * @dest: Is an input value
 * @src: Is an input value
 *
 * Return: void
 */

char *_strcat(char *dest, char *src)
{
	int i = 0;
	int j = 0;

		while (dest[i] != '\0')
		{
			i++;
		}

		while (src[j] != '\0')
		{
			dest[i] = src[j];
			i++;
			j++;
		}

		dest[i] = '\0';

		return (dest);
}
